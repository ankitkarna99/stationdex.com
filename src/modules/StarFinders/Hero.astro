---
import Icon from "@/elements/Icon.astro";
---

<div class="star finders hero">
  <div class="inner">
    <div class="wrapper">
      <div class="component">
        <div class="content">
          <Icon variant="title-shield" className="shield" />

          <h1>
            StationDEX <span>Starfinders Program</span>
          </h1>

          <p>Mission #1</p>

          <div class="cta">
            <a href="https://app.stationdex.com/" target="_blank" class="swap"
              >Swap Tokens</a
            >
            <a
              href="https://app.stationdex.com/pools"
              target="_blank"
              class="add">Add Liquidity</a
            >
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
